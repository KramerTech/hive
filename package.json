{
	"name": "Hive",
	"browserslist": [
		"last 2 Chrome versions"
	],
	"scripts": {
		"start": "parcel serve index.html",
		"build": "rm -fr www && mkdir www && parcel build --no-source-maps --dist-dir www index.html",
		"deps": "npm i && npm i -g concurrently parcel-bundler reload",
		"server": "ts-node-dev server/server.ts"
	},
	"devDependencies": {
		"@types/node": "^10.12.18",
		"@types/ws": "^6.0.1",
		"concurrently": "^5.1.0",
		"cssnano": "^4.1.10",
		"reload": "^3.0.4",
		"typescript": "^3.2.4"
	},
	"dependencies": {
		"copy-to-clipboard": "^3.3.1",
		"peerjs": "^1.3.2",
		"pure-rand": "^1.6.2",
		"toastify-js": "^1.9.1",
		"ws": "^7.0.0"
	}
}
